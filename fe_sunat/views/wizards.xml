<?xml version="1.0" encoding="utf-8"?>
<odoo>
	
	<record id="view_invoice_sunat_wizard" model="ir.ui.view">
		<field name="name">invoice.sunat.wizard.form</field>
		<field name="model">invoice.sunat.wizard</field>
		<field name="arch" type="xml">
			<form>
				<p class="oe_grey">
					Confirmar Factura
				</p>
				<footer>
        	<button string="Confirmar Factura" name="invoice_confirm" type="object" default_focus="1" class="btn-primary"/>
        	<button string="Cancel" class="btn-default" special="cancel"/>
     		</footer>
			</form>
		</field>
	</record>

</odoo>
